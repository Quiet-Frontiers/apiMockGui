{
  "name": "api-mock-gui",
  "version": "1.0.0",
  "description": "A GUI library for mocking API responses with case management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auto": {
      "import": "./dist/auto.js",
      "types": "./dist/auto.d.ts"
    },
    "./dist/styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run build:css",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css --minify",
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "api",
    "mock",
    "gui",
    "testing",
    "development",
    "msw"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.0",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "lucide-react": "^0.263.1",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0",
    "msw": "^2.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}